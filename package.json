{
  "name": "evadesign-mockup",
  "version": "1.0.0",
  "description": "Eva Design Mockup - Modern website template combining shadcn/ui and Aceternity UI components",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
