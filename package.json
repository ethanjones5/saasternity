    "clean": "rm -rf .next node_modules/.cache",
    "setup": "npm install && npm run build",
    "add:shadcn": "npx shadcn@latest add",
    "generate:site": "node scripts/generate-site.js",
    "test:build": "npm run build && npm start"
  },
  "dependencies": {
